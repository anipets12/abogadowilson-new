{
  "version": 1,
  "include": [
    "/*"
  ],
  "exclude": [],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/favicon.svg",
      "dest": "/favicon.svg"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/api/.*",
      "dest": "/index.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/.*\\.(js|css|png|jpg|gif|svg|ico|json|woff|woff2|ttf|eot)",
      "dest": "/$&"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
